NameVirtualHost *:<%= port %>
<VirtualHost *:<%= port %>>
  ServerName <%= name %>
  ServerAlias <%= serveralias %> 
 DocumentRoot <%= docroot %>
 <Directory <%= docroot %>>
   Options Indexes FollowSymLinks MultiViews
   AllowOverride None
   Order allow,deny
   allow from all
 </Directory>
  ErrorLog /var/log/httpd/<%= name %>_error.log
  LogLevel warn
  CustomLog /var/log/httpd/<%= name %>_access.log combined
  ServerSignature On
</VirtualHost> 
